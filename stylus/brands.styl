/* brand silos */
body.jcb
    colors = '#breadcrumb nav i:before,
    div.keyStats span,
    div.catList i:before,
    .brand,
    h1, 
    h2,
    main a.ctaButton:hover'
    {colors}
        color: jcb !important
    borders = 'main hr,
    .thumbImg a.activeLink,
    div.keyStats p,
    main a.ctaButton,
    table.rowResponsive tbody tr '
    {borders}
        border-color: jcb !important 
    bgs = 'main a.ctaButton,
    table.rowResponsive thead '
    {bgs}
        background-color: jcb 
    brandHR = 'main hr'
    {brandHR}
        border-bottom-width: 3px

