amazone = #e97714 //Amazone
asv = #a1c02f //ASV
mahindra = #e41937 //mahindra
yeomans-plow = #ff0f00 //yeomans plow 


/* brandCarousel  */
div.brandCarousel a.jcb i
    color: jcb
div.brandCarousel a.jcb
    border-color: jcb !important

div.brandCarousel a.massey-ferguson
    border-color: massey-ferguson !important
div.brandCarousel a.massey-ferguson i
    color: massey-ferguson

div.brandCarousel a.fendt
    border-color: fendt !important
div.brandCarousel a.fendt i
    color: fendt

div.brandCarousel a.dressta
    border-color: dressta !important
div.brandCarousel a.dressta i
    color: dressta

div.brandCarousel a.iseki
    border-color: iseki !important
div.brandCarousel a.iseki i
    color: iseki

div.brandCarousel a.claas
    border-color: claas !important
div.brandCarousel a.claas i
    color: claas    

div.brandCarousel a.amazone
    border-color: amazone !important
div.brandCarousel a.amazone i
    color: amazone

div.brandCarousel a.asv
    border-color: asv !important
div.brandCarousel a.asv i
    color: asv
    
div.brandCarousel a.mahindra
    border-color: mahindra !important
div.brandCarousel a.mahindra i
    color: mahindra

div.brandCarousel a.yeomans-plow
    border-color: yeomans-plow !important
div.brandCarousel a.yeomans-plow i
    color: yeomans-plow    


// branded CTA outside brand pages
.ctaButton.jcb
    background-color: jcb !important
    border-color: jcb !important
.ctaButton.massey-ferguson
    background-color: massey-ferguson !important
    border-color: massey-ferguson !important
.ctaButton.fendt
    background-color: fendt !important
    border-color: fendt !important
.ctaButton.dressta
    background-color: dressta !important
    border-color: dressta !important
.ctaButton.iseki
    background-color: iseki !important
    border-color: iseki !important
.ctaButton.claas
    background-color: claas !important
    border-color: claas !important
.ctaButton.amazone
    background-color: amazone !important
    border-color: amazone !important
.ctaButton.asv
    background-color: asv !important
    border-color: asv !important
.ctaButton.mahindra
    background-color: mahindra !important
    border-color: mahindra !important
.ctaButton.yeomans-plow 
    background-color: yeomans-plow  !important  
